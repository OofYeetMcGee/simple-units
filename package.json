        "@types/sprintf-js": "^1.1.2",
        "@typescript-eslint/eslint-plugin": "^4.28.1",
        "@typescript-eslint/parser": "^4.28.1",
        "convert": "^2.1.1",
        "convert-units": "^2.3.4",
        "dependency-cruiser": "^10.1.1",
        "eslint": "^7.29.0",
        "source-map-support": "^0.5.19",
        "ts-node": "^10.0.0",
        "typedoc": "^0.21.6",
        "typedoc-plugin-pages": "^1.1.0",
        "typescript": "^4.3.5",
